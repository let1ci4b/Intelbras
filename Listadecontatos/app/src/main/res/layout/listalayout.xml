<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layoutContato1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="150dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:visibility="gone"
        tools:visibility="visible">

        <ImageView
            android:id="@+id/iconPhone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/iconphone"
            android:layout_marginStart="80dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" />

        <TextView
            android:id="@+id/nomeContato1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            android:layout_marginEnd="80dp"
            tools:text="contato 1"
            android:textColor="#000000"
            android:textSize="16sp"
            app:layout_constraintBottom_toTopOf="@+id/telefoneContato1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/iconPhone"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/telefoneContato1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            android:layout_marginEnd="80dp"
            android:textColor="#000000"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/iconPhone"
            app:layout_constraintTop_toBottomOf="@+id/nomeContato1"
            tools:text="telefone 1" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layoutContato2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/layoutContato1"
        android:visibility="gone"
        tools:visibility="visible" >

        <ImageView
            android:id="@+id/iconPhone2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/iconphone"
            android:layout_marginStart="80dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" />

        <TextView
            android:id="@+id/nomeContato2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:textSize="16sp"
            android:layout_marginStart="15dp"
            android:layout_marginEnd="80dp"
            app:layout_constraintBottom_toTopOf="@+id/telefoneContato2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/iconPhone2"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="contato 2" />

        <TextView
            android:id="@+id/telefoneContato2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            tools:text="telefone 2"
            android:textColor="#000000"
            android:textSize="16sp"
            android:layout_marginStart="15dp"
            android:layout_marginEnd="80dp"
            app:layout_constraintStart_toEndOf="@id/iconPhone2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/nomeContato2"
            app:layout_constraintBottom_toBottomOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layoutContato3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/layoutContato2"
        android:visibility="gone"
        tools:visibility="visible" >

        <ImageView
            android:id="@+id/iconPhone3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/iconphone"
            android:layout_marginStart="80dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" />

        <TextView
            android:id="@+id/nomeContato3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            tools:text="contato 3"
            android:textColor="#000000"
            android:textSize="16sp"
            android:layout_marginStart="15dp"
            android:layout_marginEnd="80dp"
            app:layout_constraintStart_toEndOf="@id/iconPhone3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/telefoneContato3" />

        <TextView
            android:id="@+id/telefoneContato3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            tools:text="telefone 3"
            android:textColor="#000000"
            android:textSize="16sp"
            android:layout_marginStart="15dp"
            android:layout_marginEnd="80dp"
            app:layout_constraintStart_toEndOf="@id/iconPhone3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/nomeContato3"
            app:layout_constraintBottom_toBottomOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:id="@+id/btnReturn"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="80dp"
        android:layout_marginTop="350dp"
        android:layout_marginEnd="80dp"
        android:backgroundTint="#000000"
        android:text="RETORNAR"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>